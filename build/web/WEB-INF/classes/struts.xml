<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!--<include file="example.xml"/>-->
    <!-- Configuration for the default package. -->
    <package name="OnlineHealthCareSystem" extends="struts-default">
    
    <!-- Login -->
        <action name="login" class="com.myapp.struts.LoginAction">
            <result name="success">success.jsp</result>
            <result name="input">login.jsp</result>
        </action>
        
     <!-- Add a new doctor -->
        <action name="AddDoctor" class="com.myapp.struts.AddDoctorAction">
            <result name="success">success.jsp</result>
            <result name="input">regdoc.jsp</result>
        </action>
     
     <!-- show a doctor -->
        <action name="ShowDoctor" class="com.myapp.struts.ShowDoctorAction">
            <result name="success">doctor.jsp</result>
            <result name="input">doctor.jsp</result>
        </action>
       
        
    </package>
</struts>
